{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "require-jsdoc": 0
    }
}
